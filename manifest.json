{
  "manifest_version":2,
  "name": "Record click bot",
  "description": "record clicks and replay",
  "version":"2.0",
  "icons": {
    "128":"icon_128.png"
  },
  "browser_action":{
    "default_cion":"icon_128.png",
    "default_popup":"index.html"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["index.js"]
    }
  ],
  "background": {
    "scripts":["background.js"]
  },
  "permissions":["activeTab"]
}